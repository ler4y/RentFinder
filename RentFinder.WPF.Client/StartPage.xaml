<Page x:Class="RentFinder.WPF.Client.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RentFinder.WPF.Client"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="StartPage">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="newSearchBtn" Margin="5">New Search</Button>
            <Button x:Name="openLastBtn"  Margin="5" Click="openLastBtn_Click">Open Last</Button>
        </StackPanel>
    </Grid>
</Page>
